num = int(input("Enter a 4 digit number: ")) # SyntaxError
ones = num % 10 # TypeError
print("Ones digit is", ones) # SyntaxError
tens = (num // 10) % 10 # SyntaxError
print("Tens digit is", tens) # SyntaxError
hunds = num % 1000//100 # LogicError
print("Hundreds digit is", hunds) # LogicError
thous = num // 1000 # LogicError
print("Thousands digit is", thous) #Syntax Error
rev = (1000*ones)+(100*tens)+(10*hunds)+(thous) #TypeError
print("Original number reversed is ", rev) #SyntaxError
